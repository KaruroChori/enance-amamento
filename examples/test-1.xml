<?xml version="1.0" encoding="UTF-8"?>
<!--Example of a low level tree structure for an SDF forest-->
<forest>
    <join label="$">
        <join>
            <sphere radius="5">
                <cfg idx="1" />
            </sphere>
            <translate offset="5|0|0">
                <sphere radius="3">
                    <cfg idx="2" />
                </sphere>
            </translate>
        </join>
        <join>
            <join>
                <sphere radius="5">
                    <cfg idx="1" />
                </sphere>
                <translate offset="5|0|0">
                    <sphere radius="3">
                        <cfg idx="2" />
                    </sphere>
                </translate>
            </join>
            <join>
                <join>
                    <sphere radius="5">
                        <cfg idx="1" />
                    </sphere>
                    <translate offset="5|0|0">
                        <sphere radius="3">
                            <cfg idx="2" />
                        </sphere>
                    </translate>
                </join>
                <join>
                    <join>
                        <sphere radius="5">
                            <cfg idx="1" />
                        </sphere>
                        <translate offset="5|0|0">
                            <sphere radius="3">
                                <cfg idx="2" />
                            </sphere>
                        </translate>
                    </join>
                    <translate offset="5|5|0">
                        <sphere radius="3">
                            <cfg idx="3" />
                        </sphere>
                    </translate>
                </join>
            </join>
        </join>
    </join>
</forest>